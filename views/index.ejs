<h4 class="text-white"><strong>Dashboard</strong></h4>
<hr class="text-white">

<h6 class="text-white">Recently added books:</h6>
<div class="d-flex justify-content-grid align-items-grid text-white mt-2">
  <% books.slice(0, Math.max(5, books.length)).forEach(book => { %>
  <a type="button" href="/books/<%= book.id %>" style="margin: 10px;">
    <img height="150" width="100" src="<%= book.coverImagePath %>">
  </a>
  <% }) %>
</div>